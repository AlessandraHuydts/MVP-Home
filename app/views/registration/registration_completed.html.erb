<div class="phone-page-registration-completed">
  <div class="welcome-message">
    <h1>Registration completed</h1>
  </div>
  <div class="button-start-swiping">
    <%= simple_form_for :search, url:  location_set_user_path(current_user), method: :post do |f| %>
      <%= f.input :query, as: :hidden %>
      <%= f.submit "Get swiping!" %>
    <% end %>
  </div>
</div>